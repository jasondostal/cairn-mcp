{
  "hooks": {
    "session-start": {
      "command": "CAIRN_URL=http://localhost:8000 /path/to/cairn/examples/hooks/adapters/cursor/session-start.sh",
      "timeout": 15
    },
    "after-mcp-execution": {
      "command": "/path/to/cairn/examples/hooks/adapters/cursor/after-mcp-execution.sh",
      "timeout": 5
    },
    "session-end": {
      "command": "CAIRN_URL=http://localhost:8000 /path/to/cairn/examples/hooks/adapters/cursor/session-end.sh",
      "timeout": 30
    }
  }
}
